{
  "name": "reach-ui-website",
  "description": "The accessible foundation of your React apps and design systems",
  "private": true,
  "repository": "https://github.com/reach/reach-ui",
  "author": "React Training <hello@reacttraining.com>",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "yarn dev",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "@philpl/buble": "^0.19.7",
    "@reach/accordion": "file:../packages/accordion",
    "@reach/alert": "file:../packages/alert",
    "@reach/alert-dialog": "file:../packages/alert-dialog",
    "@reach/auto-id": "file:../packages/auto-id",
    "@reach/checkbox": "file:../packages/checkbox",
    "@reach/combobox": "file:../packages/combobox",
    "@reach/component-component": "file:../packages/component-component",
    "@reach/descendants": "file:../packages/descendants",
    "@reach/dialog": "file:../packages/dialog",
    "@reach/disclosure": "file:../packages/disclosure",
    "@reach/listbox": "file:../packages/listbox",
    "@reach/menu-button": "file:../packages/menu-button",
    "@reach/popover": "file:../packages/popover",
    "@reach/portal": "file:../packages/portal",
    "@reach/rect": "file:../packages/rect",
    "@reach/skip-nav": "file:../packages/skip-nav",
    "@reach/slider": "file:../packages/slider",
    "@reach/tabs": "file:../packages/tabs",
    "@reach/tooltip": "file:../packages/tooltip",
    "@reach/utils": "file:../packages/utils",
    "@reach/visually-hidden": "file:../packages/visually-hidden",
    "@reach/window-size": "file:../packages/window-size",
    "core-js": "^3.6.5",
    "gatsby": "^2.23.17",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-google-analytics": "^2.3.9",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-mdx": "^1.2.22",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-sass": "^2.3.10",
    "gatsby-plugin-sharp": "^2.6.17",
    "gatsby-remark-autolink-headers": "^2.3.10",
    "gatsby-remark-emoji": "^0.0.3",
    "gatsby-remark-images": "^3.3.17",
    "gatsby-remark-slug": "^0.1.0",
    "gatsby-source-filesystem": "^2.3.17",
    "gatsby-transformer-react-docgen": "^5.2.10",
    "gatsby-transformer-sharp": "^2.5.10",
    "match-sorter": "^4.1.0",
    "node-sass": "^4.14.1",
    "postcss-preset-env": "^6.7.0",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^8.0.27",
    "recondition": "^1.2.4",
    "use-throttle": "^0.0.3"
  },
  "resolutions": {
    "xstate": "4.10.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 2%",
    "maintained node versions",
    "IE 11"
  ],
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "__DEV__": "readonly"
    },
    "rules": {
      "no-unused-vars": [
        1,
        {
          "args": "after-used",
          "ignoreRestSiblings": true,
          "argsIgnorePattern": "^(event|_)$"
        }
      ],
      "import/first": 0,
      "jsx-a11y/no-static-element-interactions": [
        1,
        {
          "handlers": [
            "onClick",
            "onMouseDown",
            "onMouseUp",
            "onKeyPress",
            "onKeyDown",
            "onKeyUp"
          ]
        }
      ]
    }
  }
}
