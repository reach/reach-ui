    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lerna": "^3.18.3",
    "lerna-changelog": "^0.8.2",
    "pepjs": "0.5.2",
    "prettier": "^1.18.2",
    "pretty-quick": "2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "workspaces": [
    "packages/*"
  ],
