  "resolutions": {
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.6"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "changelog": {
    "labels": {
      "Type: Breaking Change": "üî® Breaking Changes",
      "Type: Bug": "üêõ Bugfixes",
      "Type: Documentation": "üìù Documentation",
      "Type: Feature": "üöÄ Features",
      "Type: Enhancement": "‚ú® Enhancements",
      "Type: Housekeeping": "üè° Housekeeping"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": "readonly"
    },
    "rules": {
      "no-unused-vars": [
        1,
        {
          "args": "after-used",
          "ignoreRestSiblings": true,
          "argsIgnorePattern": "^(event|_)$"
        }
      ],
      "import/first": 0,
      "jsx-a11y/no-static-element-interactions": [
        1,
        {
          "handlers": [
            "onClick",
            "onMouseDown",
            "onMouseUp",
            "onKeyPress",
            "onKeyDown",
            "onKeyUp"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "packages/*/dist"
  ],
  "prettier": {
    "singleQuote": false
  }
}
