{
  "name": "reach-ui",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "build:changed": "lerna run build --parallel --since origin/master",
    "build": "lerna run build --parallel",
    "precommit": "pretty-quick --staged",
    "release": "lerna run test --since origin/master && yarn build:changed && lerna publish --since origin/master"
  },
  "license": "MIT",
  "repository": "https://github.com/reach/reach-ui",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "css-loader": "^2.1.1",
    "eslint": "5.16.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.6.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "gzip-size": "^5.1.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^24.7.1",
    "lerna": "^3.13.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "pretty-bytes": "^5.1.0",
    "pretty-quick": "^1.6.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "style-loader": "^0.23.1"
  },
  "prettier": {},
  "husky": {
    "hooks": {
      "pre-push": "lerna run lint"
    }
  }
