{
  "name": "reach-ui",
  "description": "The accessible foundation of your React apps and design systems",
  "private": true,
  "author": "React Training <hello@reacttraining.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reach/reach-ui.git"
  },
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "cross-env NODE_ENV=test CI=true tsdx test packages",
    "test:root": "cross-env NODE_ENV=test CI=true tsdx test",
    "test:watch": "cross-env NODE_ENV=test tsdx test packages",
    "test:coverage": "yarn test --coverage",
    "build:changed": "lerna run build --since origin/master",
    "build": "lerna run build --stream",
    "ver": "lerna version --no-push",
    "changes": "dotenv lerna-changelog",
    "clean": "git clean -e '!/.env' -e '!/website-deploy-key' -e '!/website-deploy-key.pub' -fdX .",
    "lint": "eslint .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@reach/router": "^1.3.3",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.0.0",
    "@types/escape-regexp": "^0.0.0",
    "@types/highlight-words-core": "^1.2.0",
    "@types/invariant": "^2.2.31",
    "@types/jest": "^25.1.4",
    "@types/match-sorter": "^4.0.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/sinon": "^7.5.2",
    "@types/styled-components": "^5.0.1",
    "@types/tabbable": "^3.1.0",
    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "autoprefixer": "^9.7.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dev-expression": "^0.2.2",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-axe": "^3.4.0",
    "lerna": "^3.20.2",
    "lerna-changelog": "^1.0.1",
    "match-sorter": "^4.0.2",
    "patch-package": "^6.2.1",
    "pepjs": "^0.5.2",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.0",
    "react-dom": "^16.13.0",
    "react-is": "^16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^16.13.0",
    "sinon": "^8.1.1",
    "styled-components": "^5.0.1",
    "ts-loader": "^6.2.1",
    "tsdx": "^0.12.3",
    "typescript": "^3.8.3",
    "use-throttle": "^0.0.3"
  },
  "resolutions": {
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "changelog": {
    "labels": {
      "breaking-change": "üî® Breaking Changes",
      "bug": "üêõ Bugfixes",
      "docs": "üìù Documentation",
      "feature": "üöÄ Features",
      "enhancement": "‚ú® Enhancements",
      "housekeeping": "üè° Housekeeping"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": "readonly"
    },
    "rules": {
      "no-unused-vars": [
        1,
        {
          "args": "after-used",
          "ignoreRestSiblings": true,
          "argsIgnorePattern": "^(event|_)$"
        }
      ],
      "import/first": 0,
      "jsx-a11y/no-static-element-interactions": [
        1,
        {
          "handlers": [
            "onClick",
            "onMouseDown",
            "onMouseUp",
            "onKeyPress",
            "onKeyDown",
            "onKeyUp"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "packages/*/dist"
  ],
  "jest": {
    "globals": {
      "__DEV__": "boolean"
    },
    "collectCoverageFrom": [
      "packages/**/src/**/*.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\$test(.*)$": "<rootDir>/test/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "es5"
  }
}
