{
  "private": true,
  "name": "reach-ui",
  "repository": "https://github.com/reach/reach-ui",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "MODULE_FORMAT=cjs jest packages",
    "build:changed": "lerna run build --parallel --since origin/master",
    "build": "lerna run build --parallel",
    "changes": "lerna-changelog",
    "clean": "git clean -fdX .",
    "release": "lerna run test --since origin/master && yarn build:changed && lerna publish --since origin/master",
    "lint": "lerna run lint",
    "doc": "react-docgen packages/*/src/* --out www/docs/source.json --resolver findAllComponentDefinitions --pretty"
  },
  "dependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "autoprefixer": "^9.6.4",
    "babel-eslint": "10.x",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dev-expression": "^0.2.2",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "glob": "^7.1.4",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lerna": "^3.16.5",
    "lerna-changelog": "^0.8.2",
    "pepjs": "0.5.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-docgen": "^4.1.1",
    "react-dom": "^16.10.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "changelog": {
    "labels": {
      "breaking-change": "BREAKING CHANGE",
      "bug": "BUGFIX",
      "docs": "DOCUMENTATION",
      "enhancement": "FEATURE"
    }
  }
}
